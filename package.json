{
  "name": "jest-mock-axios",
  "version": "2.1.11",
  "description": "Axios mock for Jest",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack -p",
    "deploy2npm": "npm run build && npm publish",
    "test:type": "jest -c ./jest.tsc.config.js",
    "test:coverage": "jest --coverage",
    "test": "npm run test:type && npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knee-cola/jest-mock-axios.git"
  },
  "keywords": [
    "axios",
    "jest",
    "mock"
  ],
  "author": "knee-cola <nikola.derezic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knee-cola/jest-mock-axios/issues"
  },
  "homepage": "https://github.com/knee-cola/jest-mock-axios#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^24.0.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "codecov": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-runner-tsc": "^1.4.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "jest-mock-promise": "^1.0.23"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "projects": [
      "jest.config.js",
      "jest.tsc.config.js"
    ]
  }
}
